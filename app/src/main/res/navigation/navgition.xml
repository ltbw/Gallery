<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navgition"
    app:startDestination="@id/galleryFragment">

    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.oashwe.gallery.GalleryFragment"
        android:label="fragment_gallery"
        tools:layout="@layout/fragment_gallery" >
        <action
            android:id="@+id/action_galleryFragment_to_photoItemFragment"
            app:destination="@id/photoItemFragment" />
    </fragment>
    <fragment
        android:id="@+id/photoItemFragment"
        android:name="com.oashwe.gallery.PhotoItemFragment"
        android:label="fragment_photo_item"
        tools:layout="@layout/fragment_photo_item" />
</navigation>